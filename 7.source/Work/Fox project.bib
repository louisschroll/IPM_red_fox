@article{abrams_2000,
  title = {{The Evolution of Predator-Prey Interactions: Theory and Evidence}},
  shorttitle = {{The Evolution of Predator-Prey Interactions}},
  author = {Abrams, Peter A.},
  year = {2000},
  month = nov,
  journal = {Annual Review of Ecology, Evolution, and Systematics},
  volume = {31},
  number = {Volume 31, 2000},
  pages = {79--105},
  publisher = {Annual Reviews},
  issn = {1543-592X, 1545-2069},
  doi = {10.1146/annurev.ecolsys.31.1.79},
  url = {https://www.annualreviews.org/content/journals/10.1146/annurev.ecolsys.31.1.79},
  urldate = {2024-09-25},
  abstract = {▪ Abstract Recent theories regarding the evolution of predator-prey interactions is reviewed. This includes theory about the dynamics and stability of both populations and traits, as well as theory predicting how predatory and anti-predator traits should respond to environmental changes. Evolution can stabilize or destabilize interactions; stability is most likely when only the predator evolves, or when traits in one or both species are under strong stabilizing selection. Stability seems least likely when there is coevolution and a bi-directional axis of prey vulnerability. When population cycles exist, adaptation may either increase or decrease the amplitude of those cycles. An increase in the defensive ability of prey is less likely to produce evolutionary counter-measures in its partner than is a comparable increase in attack ability of the predator. Increased productivity may increase or decrease offensive and defensive adaptations. The apparent predominance of evolutionary responses of prey to predators over those of predators to prey is in general accord with equilibrium theory, but theory on stability may be difficult to confirm or refute. Recent work on geographically structured populations promises to advance our understanding of the evolution of predator-prey interactions.},
  langid = {french},
  file = {C:\Users\schroll\Zotero\storage\PHJ277ZB\annurev.ecolsys.31.1.html}
}

